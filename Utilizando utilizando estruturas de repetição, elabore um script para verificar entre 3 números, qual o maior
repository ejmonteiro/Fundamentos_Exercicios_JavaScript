var num1 = prompt (`digite um valor`);
    var num2 = prompt (`digite um outro valor`);
    var num3 = prompt (`digite o ultimo valor`);

    if(num1>num2 && num1>num3){
       alert('Maior valor é '+ num1);

    }else if(num2>num3 && num2>num1){
        alert(`Maior valor é` + num2);

    }else if(num3> num2 && num3>num1){
        alert(`Maior valor é` + num3);

    }else{
        alert(`Nao foi possivel informar qual maior numero`);
    } 
